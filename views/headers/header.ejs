    <style>
        .options i.standard,
        .options .fa {
            height: 1em;
            margin: 0;
            padding: 0.5em;
            width: 1em;
            cursor: pointer;
        }

        .options i.standard,
        .options .fa:before {
            margin: 0;
            border: 1px solid transparent;
            display: inline-block;
            height: 1em;
            width: 1em;
            text-align: center;
            padding: 2px;
        }

        .options i.standard {
            font-style: normal;
            font-weight: bold;
            font-family: sherif;
            padding-bottom: 0.1em;
            line-height: 0.9em;
            margin-left: 0.35em;
        }

        .options i.standard:hover,
        .options .fa:hover:before {
            border: 1px solid #369;
            border-radius: 100%;
            transform: scale(3);
            -webkit-transform: scale(3);
            background: #fff;
        }

        .ol-popup h1 {
            font-size: 1em;
            background-size: 2em;
            padding-left: 3em;
        }

        .ol-zoom {
            top: 4.5em
        }

        .ol-zoomslider {
            background-color: transparent;
            top: 6.3em;            
        }

        .ol-zoom .ol-zoom-out {
            margin-top: 204px;            
        }

        .ol-touch .ol-zoom .ol-zoom-out {
            margin-top: 212px;
        }
        .ol-touch .ol-zoomslider {
            top: 2.75em;
        }

        .ol-zoom-in.ol-has-tooltip:hover [role=tooltip],
        .ol-zoom-in.ol-has-tooltip:focus [role=tooltip] {
            top: 3px;
        }

        .ol-zoom-out.ol-has-tooltip:hover [role=tooltip],
        .ol-zoom-out.ol-has-tooltip:focus [role=tooltip] {
            top: 232px;
        }

        .layer-switcher {
            right: .2em;
            top: .5em;
        }
        
        #heatmap-select {
            position: absolute;
            top: 60px;
            left: 20px;
            width: 130px;
            height: 30px;
            border-radius: 5px;
        }
        .cbutton {
            background-color: #008CBA; /* Green */
            border: none;
            color: white;
            padding: 5px;
            width: 150px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 18px;  
            cursor: pointer;
            border-radius: 4px;
        }

        /* popup style */
        .ol-popup {
            /* max-width:300px; */        
            min-width:100px;
            min-height:1em;
        }
        /* Image on popup */
        .ol-popup img {        
            margin: 1em 1em 1em 1em;
            max-width: 500px;
            max-height: 300px;
        }
        .ol-popup.default {
            border-radius: 20px;
        }
        .cus-popup-title {
            font-size: large;
            font-weight: bold;
            text-align: center;
            margin: .5em 1em .2em 1em;
        }
        .cus-popup-body {
            font-size: medium;
            /* font-weight: bold; */
            /* text-align: center; */
            margin-left: 1em;
        }

